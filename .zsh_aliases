alias crontab-e="$EDITOR ~/.crontab && crontab ~/.crontab"
alias l="ls -F"
ago() {
ag "$@" &&
file=$(ag -l --nobreak "$@" | fzf --height 20%)
$EDITOR $file
}
edit() {
    file=$(git ls-files | fzf --height 20%) &&
    $EDITOR $file
}
nav() {
folder=$(g ls-files | sed 's/\/[^/]*$//' | fzf --height 20%) &&
cd $folder
}
alias chat=profanity
